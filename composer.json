    "require": {
        "php": ">7.3.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "inhere/console": "dev-master",
        "inhere/sroute": "dev-master",
        "cebe/markdown": "^1.2",
        "php-comp/http-client": "dev-master",
        "php-comp/http-message": "^1.4"
    },
    "require-dev": {
